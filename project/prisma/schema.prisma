// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Project {
  id                  Int      @id @default(autoincrement())
  team                String
  price               Decimal
  budget              Decimal
  deadline            DateTime
  description         String
  registeredWorkHours Int
  techSkills          String
  programmingLanguages String
  roles               String
  yearsOfExperience   Int
  stakeholder         String
  company             String
}